{
  "name": "discord_bot_mac",
  "version": "1.0.0",
  "description": "this is a discord bot made to manage and work over the project of mac HUB",
  "main": "setup.js",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --max-warnings 0 && prettier src --check",
    "start-server": "node -r dotenv/config  compiledJsDirectories/setup.js",
    "start-client": "node -r dotenv/config compiledJsDirectories/server/index.js",
    "start": "concurrently \"npm run start-server\" \"npm run start-client\"",
    "test": "echo 'no tests yet'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/mongodb": "^4.0.7",
    "axios": "^1.4.0",
    "discord.js": "^14.11.0",
    "dotenv": "16.1.4",
    "express": "^4.18.2",
    "mongodb": "^5.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "concurrently": "^8.2.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "^2.8.8",
    "typescript": "5.1.3"
  }
}
